<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BusinessMember">
	<select id="selectByMember" parameterType="int" resultType="BusinessMember">
		select * from business_member where member_idx=#{memberIdx}
	</select>
</mapper>